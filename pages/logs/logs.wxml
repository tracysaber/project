<!--logs.wxml-->
<view class="container log-list" style="background-image: url('https://iieluqu.oss-cn-beijing.aliyuncs.com/images/1.jpg');">
  <!-- <block wx:for="{{logs}}" wx:for-item="log"> -->
  <block>
    <text class="userinfo-nickname">考生：{{userInfo.nickName}}</text>
  </block>

  <block>
    <text class="log-item">{{index}}. {{subject}}</text>
  </block>
  <block>
    <!-- <a href="buttonClick" class="weui-btn weui-btn_primary">页面主操作 Normal</a>   -->
    <button id="1" bindtap="buttonClick">{{choice1}}</button>
  </block>
  <block>
    <button id="2" bindtap="buttonClick">{{choice2}}</button>
  </block>
  <text>已经答对{{scores}}</text>
</view>
